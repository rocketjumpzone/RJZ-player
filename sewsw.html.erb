<div class="w3-auto w3-container">
  <div class="w3-row">
    <div class="w3-col l2 m2">
      <h1><%= current_page.data.name %></h1>
      <%= partial "partials/player/sewsw/avatar" %>
      <div class="player-social">
          <a href="https://www.youtube.com/@se7052" target="_blank" rel="noopener noreferrer">
              <i class="fab fa-lg fa-youtube" aria-hidden="true"></i>
              <span class="sr-only">YouTube</span>
          </a>
          <a href="https://steamcommunity.com/id/rasheh/" target="_blank" rel="noopener noreferrer">
              <i class="fab fa-lg fa-steam" aria-hidden="true"></i>
              <span class="sr-only">Steam</span>
          </a>
          <div class="w3-tooltip mobile-tooltip w3-hide-small" style="display:inline">
              <i class="fab fa-lg fa-discord" aria-hidden="true"></i>
              <span class="sr-only">Discord</span>
              <span style="position:absolute;left:40px;bottom:-25px;width:200px" class="w3-text w3-tag w3-padding w3-animate-opacity w3-round"><%= current_page.data.discord_id %></span>
